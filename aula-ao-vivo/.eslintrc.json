{
    "extends": "trybe-frontend",
    "rules": {
      "no-alert": "off",
      "no-console": "off",
      "prefer-arrow-callback": "off",
      "func-names": "off",
      "no-unused-vars": "warn",
      "no-use-before-define": "off",
      "no-magic-numbers": "off"
    },
    "ignorePatterns": [
      "**/*.html"
    ]
  }